{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "baseUrl": "./",
    "paths": {
      "@src/*": [
        "./src/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@assets": [
        "./src/assets"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@modules/*": [
        "./src/modules/*"
      ],
      "@modules": [
        "./src/modules"
      ],
      "@configs/*": [
        "./src/configs/*"
      ],
      "@configs": [
        "./src/configs"
      ],
      "@contexts/*": [
        "./src/contexts/*"
      ],
      "@contexts": [
        "./src/contexts"
      ],
      "@global/*": [
        "./src/global/*"
      ],
      "@global": [
        "./src/global"
      ],
      "@helpers/*": [
        "./src/helpers/*"
      ],
      "@helpers": [
        "./src/helpers"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@hooks": [
        "./src/hooks"
      ],
      "@screens/*": [
        "src/screens/*"
      ],
      "@screens": [
        "src/screens"
      ],
      "@routes/*": [
        "./src/routes/*"
      ],
      "@routes": [
        "./src/routes"
      ],
      "@services/*": [
        "./src/services/*"
      ],
      "@services": [
        "./src/services"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@styles": [
        "./src/styles"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@utils": [
        "./src/utils"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ],
  "extends": "expo/tsconfig.base"
}

